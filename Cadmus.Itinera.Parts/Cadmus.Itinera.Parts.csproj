<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <TargetFramework>net7.0</TargetFramework>
	<Nullable>enable</Nullable>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus components for the Itinera project.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>3.0.4</Version>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageTags>Cadmus;itinera parts</PackageTags>
    <FileVersion>3.0.4</FileVersion>
    <AssemblyVersion>3.0.4</AssemblyVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>.\bin\Debug\netstandard2.0\Cadmus.Itinera.Parts.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Codicology.Parts" Version="3.0.1" />
    <PackageReference Include="Cadmus.Core" Version="5.0.0" />
    <PackageReference Include="Cadmus.Mat.Bricks" Version="4.0.0" />
    <PackageReference Include="Cadmus.Prosopa.Bricks" Version="1.0.0" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="4.0.0" />
    <PackageReference Include="Fusi.Antiquity" Version="3.0.0" />
  </ItemGroup>

</Project>
