<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus components for the Itinera project.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>2.0.5</Version>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageTags>Cadmus;itinera parts</PackageTags>
    <FileVersion>2.0.5</FileVersion>
    <AssemblyVersion>2.0.5</AssemblyVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>.\bin\Debug\netstandard2.0\Cadmus.Itinera.Parts.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Codicology.Parts" Version="1.1.3" />
    <PackageReference Include="Cadmus.Core" Version="2.5.2" />
    <PackageReference Include="Cadmus.Mat.Bricks" Version="1.0.0" />
    <PackageReference Include="Cadmus.Prosopa.Bricks" Version="1.0.0" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="2.2.2" />
    <PackageReference Include="Fusi.Antiquity" Version="1.2.0" />
  </ItemGroup>

</Project>
